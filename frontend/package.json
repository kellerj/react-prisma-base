{
  "name": "react-prisma-base-frontend",
  "version": "1.0.0",
  "description": "Application React Front End",
  "main": "server.js",
  "scripts": {
    "dev": "DOTENV_CONFIG_PATH=../backend nodemon server.js",
    "debug": "DOTENV_CONFIG_PATH=../backend nodemon --inspect server.js",
    "build": "DOTENV_CONFIG_PATH=../backend next build",
    "start": "node server.js",
    "doc": "jsdoc -c ./jsdoc.config.js",
    "test": "NODE_ENV=test jest --coverage",
    "test:watch": "NODE_ENV=test jest --watch",
    "sonar": "sonar-scanner -Dsonar.projectBaseDir=.. -Dsonar.host.url=$SONAR_URL -Dsonar.login=$SONAR_USER -Dsonar.password=$SONAR_PASSWORD",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "apollo-boost": "^0.4.4",
    "apollo-cache": "^1.3.2",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.10.6",
    "bootstrap": "^4.4.1",
    "content-security-policy-builder": "^2.1.0",
    "date-fns": "^2.8.1",
    "dotenv-expand": "^5.1.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "isomorphic-unfetch": "^3.0.0",
    "json-colorizer": "^2.2.1",
    "json-stringify-safe": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash.debounce": "^4.0.8",
    "next": "^9.1.5",
    "next-runtime-dotenv": "^1.2.0",
    "next-with-apollo": "^4.3.0",
    "node-sass": "^4.13.0",
    "nprogress": "^0.2.0",
    "passport": "^0.4.1",
    "passport-custom": "^1.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-adopt": "^0.6.0",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.12.0",
    "react-transition-group": "^4.3.0",
    "reactstrap": "^8.2.0",
    "styled-components": "^4.4.1",
    "waait": "^1.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "casual": "^1.5.19",
    "dotenv-cli": "^3.1.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "graphql-tools": "^4.0.6",
    "jest": "^24.9.0",
    "jest-runner-eslint": "^0.7.5",
    "jest-transform-graphql": "^2.1.0",
    "jsdoc": "^3.6.3",
    "jsdoc-memberof-namespace": "^2.2.0",
    "next-build-id": "^3.0.0",
    "nodemon": "^2.0.2",
    "sonar-scanner": "^3.1.0"
  },
  "//": "This is our babel config, I prefer this over a .babelrc file",
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      }
    }
  }
}
