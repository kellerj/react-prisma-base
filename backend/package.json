{
  "name": "react-prisma-base-backend",
  "version": "1.0.0",
  "description": "Business Logic for the Application",
  "scripts": {
    "start": "node src/index.js",
    "dev": "cross-env APP_CONFIG_KEY=prop-encrypt-key nodemon src/index.js",
    "debug": "cross-env APP_CONFIG_KEY=prop-encrypt-key nodemon --inspect src/index.js",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "doc": "jsdoc -c ./jsdoc.config.js",
    "db:admin": "dotenv -- prisma admin",
    "db:deploy": "dotenv -- prisma deploy",
    "db:seed": "dotenv -- prisma seed",
    "db:reset": "dotenv -- prisma reset --force && dotenv prisma seed",
    "token": "jwt --algorithm HS384 --encode -s jwt-signing-secret --jwt '{\"sub\":\"auser\"}'",
    "generate": "dotenv prisma generate",
    "postinstall": "npm run generate",
    "prisma": "docker run --name prisma --rm -d -p 4466:4466 --env PRISMA_CONFIG_PATH=/tmp/PRISMA_CONFIG.yml -v $PWD/PRISMA_CONFIG.yml:/tmp/PRISMA_CONFIG.yml prismagraphql/prisma:1.34",
    "prisma:stop": "docker stop prisma",
    "prisma:token": "dotenv node scripts/create-prisma-token.js",
    "gen-keypair": "dotenv node scripts/create-key-pair.js",
    "encrypt-config-val": "node scripts/encrypt-config-value.js"
  },
  "dependencies": {
    "apollo-server-express": "^2.9.13",
    "babel-eslint": "^10.0.3",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "2.4.3",
    "cookie-parser": "^1.4.4",
    "cross-env": "^7.0.1",
    "dotenv-cli": "^3.1.0",
    "dotenv-expand": "^5.1.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-import": "^0.7.1",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.0.4",
    "jasypt": "^1.0.1",
    "json-colorizer": "^2.2.1",
    "json-stringify-safe": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "password-validator": "^5.0.3",
    "prisma": "^1.34.10",
    "prisma-binding": "^2.3.16",
    "winston": "^3.2.1",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "casual": "^1.6.2",
    "env-cmd": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "graphql-cli": "^3.0.14",
    "jest": "^24.9.0",
    "jest-bamboo-reporter": "^1.2.1",
    "jest-runner-eslint": "^0.7.5",
    "jest-transform-graphql": "^2.1.0",
    "jsdoc": "^3.6.3",
    "jsdoc-memberof-namespace": "^2.2.0",
    "jsonwebtokencli": "^1.5.0",
    "minami": "^1.2.3",
    "nodemon": "^2.0.2"
  },
  "license": "MIT",
  "repository": {
    "url": ""
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
